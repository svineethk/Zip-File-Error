#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nxtwave/ide-exam/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/node_modules:/home/nxtwave/ide-exam/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/ide-exam/node_modules/html-minifier-terser/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nxtwave/ide-exam/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/node_modules:/home/nxtwave/ide-exam/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/ide-exam/node_modules/html-minifier-terser/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../html-minifier-terser/cli.js" "$@"
fi

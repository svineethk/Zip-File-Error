#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nxtwave/ide-exam/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint@7.32.0/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/node_modules:/home/nxtwave/ide-exam/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint-config-react-app@6.0.0_b0bbc7888db2bf28f186b3baa19dace2/node_modules/eslint-config-react-app/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint-config-react-app@6.0.0_b0bbc7888db2bf28f186b3baa19dace2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint@7.32.0/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/node_modules:/home/nxtwave/ide-exam/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint-config-react-app@6.0.0_b0bbc7888db2bf28f186b3baa19dace2/node_modules/eslint-config-react-app/node_modules:/home/nxtwave/ide-exam/node_modules/.pnpm/eslint-config-react-app@6.0.0_b0bbc7888db2bf28f186b3baa19dace2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
fi
